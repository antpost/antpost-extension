{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "SCheckPro",
  "description": "Check Keyword Position In Google",
  "version": "8.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab","tabs",
    "http://*/*",
	"https://*/*"
  ],
   "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery.js","popup.js"],
	  "run_at":"document_start"
    }
  ],
   "background": {
    "scripts": ["background.js"]
  }
}